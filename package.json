{
  "name": "freedomgpt",
  "productName": "freedomgpt",
  "version": "2.5.1",
  "description": "Our goal is to illustrate that AI Safety cannot be achieved through censorship. Let information flow freely, no matter how discomforting - because the truth shall set us free.",
  "main": "main/index.js",
  "scripts": {
    "start": "yarn build-electron && electron-forge start",
    "package": "electron-forge package",
    "make": "yarn build && electron-forge make",
    "publish": "electron-forge publish",
    "clean": "rimraf dist main renderer/out renderer/.next",
    "build": "yarn clean && yarn build-renderer && yarn build-electron",
    "build-renderer": "next build renderer",
    "build-electron": "tsc -p src",
    "make-win": "yarn make --platform=win32",
    "make-macArm": "yarn make --platform=darwin --arch=arm64",
    "make-macIntel": "yarn make --platform=darwin --arch=x64",
    "push": "yarn version --patch && git push --follow-tags"
  },
